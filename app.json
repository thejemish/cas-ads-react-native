{
  "expo": {
    "name": "cas-ads-react-native",
    "slug": "cas-ads-react-native",
    "version": "1.0.0",
    "newArchEnabled": true,
    "scheme": "cas-ads-react-native",
    "platforms": ["ios", "android"],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "./plugins/withCasAds",
        {
          "adSolution": "choice",
          "useAdvertisingId": true,
          "trackingDescription": "Your data will remain confidential and will only be used to provide you a better and personalised ad experience.",
          "adapters": [
            "appLovin",
            "audienceNetwork",
            "googleAds"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codepick.toonplay"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.codepick.toonplay"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "353a7ed2-0193-4c42-8a64-e8d851e78631"
      }
    }
  }
}
